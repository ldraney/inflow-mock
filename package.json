{
  "name": "inflow-mock",
  "version": "1.0.0",
  "description": "Generate messy databases and provide detection/fix libraries for data quality patterns",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "generate": "tsx src/generate.ts",
    "dev": "tsx watch src/generate.ts",
    "typecheck": "tsc --noEmit"
  },
  "keywords": ["data-quality", "mock-data", "sqlite", "drizzle"],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.8",
    "@types/node": "^20.10.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.0"
  },
  "dependencies": {
    "better-sqlite3": "^11.0.0",
    "drizzle-orm": "^0.29.0"
  }
}
